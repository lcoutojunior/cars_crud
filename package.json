    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa2-cors": "^2.0.6",
    "mongoose": "^6.2.4",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
